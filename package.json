    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "testRegex": "((\\.|/*.)(test))\\.js?$",
